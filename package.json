{
    "name": "ai-voice-assistant",
    "version": "1.0.0",
    "description": "AI-powered phone voice assistant for reservations using Twilio, Deepgram, and OpenAI",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "lint": "eslint src/**/*.ts",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "voice-assistant",
        "twilio",
        "openai",
        "deepgram",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@deepgram/sdk": "^3.13.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "openai": "^4.104.0",
        "pg": "^8.11.3",
        "redis": "^4.6.10",
        "twilio": "^4.19.0",
        "uuid": "^9.0.1",
        "websocket": "^1.0.35",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.25",
        "@types/morgan": "^1.9.10",
        "@types/node": "^20.19.32",
        "@types/pg": "^8.16.0",
        "@types/uuid": "^9.0.7",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.57.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
